<note version="0.3" xmlns:link="http://beatniksoftware.com/tomboy/link" xmlns:size="http://beatniksoftware.com/tomboy/size" xmlns="http://beatniksoftware.com/tomboy"><title>script</title><text xml:space="preserve"><note-content version="0.1" xmlns:link="http://beatniksoftware.com/tomboy/link" xmlns:size="http://beatniksoftware.com/tomboy/size">script

find subfolder/*.png -name &quot;difference&quot; | sed 's@subfolder/@@' &gt; xd.txt
for fileName in $(cat xd.txt)
do
	@fileName &gt;&gt; final.txt
	grep -nr subfolder/ | sed 's@subfolder/@@' &gt;&gt; final.txt
done
</note-content>
